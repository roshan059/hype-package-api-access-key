{
    "name": "hypesewa/access-api-key",
    "description": "Authorize requests to hypesewa service with API keys",
    "keywords": ["api", "api key", "authentication", "authorization", "lumen"],
    "type": "library",
    "version": "1.0.0",
    "require": {
        "php": ">=8.0.0",
        "illuminate/support": ">=5.4",
        "hypesewa/hype-vender-publish": "^1.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Hypesewa\\AccessApiKey\\": "src/"
        }
    },
    "authors": [
        {
            "name": "HypeSewa"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "dont-discover": [
                "hypesewa/access-api-key"
            ]
        }
    }
}
